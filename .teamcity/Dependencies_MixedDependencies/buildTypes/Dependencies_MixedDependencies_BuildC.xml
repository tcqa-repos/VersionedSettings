<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="aec288ab-42de-44cb-879f-11da8fa41190" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Build C</name>
  <description>Snapshot and artifact dependent on A and B</description>
  <settings>
    <options>
      <option name="artifactRules" value="**/* =&gt;" />
      <option name="checkoutMode" value="ON_SERVER" />
    </options>
    <parameters />
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_7" sourceBuildTypeId="Dependencies_MixedDependencies_BuildA" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="fileA.txt =&gt;" />
      </dependency>
      <dependency id="ARTIFACT_DEPENDENCY_8" sourceBuildTypeId="Dependencies_MixedDependencies_BuildB" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="fileB.txt =&gt;" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Dependencies_MixedDependencies_BuildA">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Dependencies_MixedDependencies_BuildB">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

